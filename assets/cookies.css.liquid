.cb-button-dismiss {
    position: absolute;
    right: 32px;
    top: 32px;
    width: 24px;
    height: 24px;
    opacity: 0.3;
    cursor: pointer;
}
@media only screen and (max-width: 576px) {
    .cb-button-dismiss {
        display: none;
    }
}
.cb-button-dismiss:hover {
    opacity: 1;
}
.cb-button-dismiss:before, .cb-button-dismiss:after {
    position: absolute;
    left: 11px;
    content: ' ';
    height: 25px;
    width: 2px;
    background-color: #333;
}
.cb-button-dismiss:before {
    transform: rotate(45deg);
}
.cb-button-dismiss:after {
    transform: rotate(-45deg);
}